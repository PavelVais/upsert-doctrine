  "require-dev": {
    "doctrine/coding-standard": "^8.0 || ^9.0",
    "phpunit/phpunit": "^8.0 || ^9.0"
  },
  "authors": [
    {
      "name": "Pavel Vais"
    }
  ],
  "minimum-stability": "dev"
