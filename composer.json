{
  "name": "pavelvais/upsert-doctrine",
  "description": "Doctrine helper for update and insert functionality",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Pavelvais\\UpsertDoctrine\\": "src/"
    }
  },
  "autoload-dev": {
